    "ncp": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "5.8.*",
    "typescript-eslint": "^8.38.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "multer": "^2.0.2",
